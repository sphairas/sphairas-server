<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
  <!--httpservlet-security-provider="iserv-imap-auth">-->
  <!--<context-root>/web</context-root>-->
  <context-root>/__web.module.context.root_</context-root>
  <!--${com.sun.aas.instanceRoot}-->
  <!--    <property name="alternatedocroot_1" value="from=/dav/* dir=/opt/glassfish-4.1/glassfish/domains/domain1/app-resources/"/>-->
  <security-role-mapping>
    <role-name>signee</role-name>
    <group-name>signees</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>unitadmin</role-name>
    <group-name>unitadmins</group-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="false">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</glassfish-web-app>
